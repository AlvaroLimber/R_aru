m1<-mean(x)
m2<-median(x)
m3<-sd(x)
m4<-max(x)
m5<-quantile(x,seq(0,1,0.2))
m6<-sum(x^2)
mm<-list(media=m1,mediana=m2,sd=m3,max=m4,quintiles=m5,x2=m6)
return(mm)
}
